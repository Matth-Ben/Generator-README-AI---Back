{
  "name": "generator-readme-ai-back",
  "version": "1.0.0",
  "description": "Backend service for Generator README AI - Generates project documentation using GPT",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "readme",
    "generator",
    "ai",
    "gpt",
    "documentation",
    "fastify"
  ],
  "author": "Matth-Ben",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "dotenv": "^16.6.1",
    "fastify": "^4.25.2",
    "firebase-admin": "^13.6.0",
    "openai": "^4.52.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
